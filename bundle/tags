!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
<C-]>	ftpluginruby.vim/ftplugin/ruby.vim	/^map <buffer><unique><silent> <C-]> :exec ":tag \/".expand("<cword>")<CR>$/;"	m
b:browsefilter	ftpluginruby.vim/ftplugin/ruby.vim	/^  let b:browsefilter = "Ruby Source Files (*.rb)\\t*.rb\\n" .$/;"	v
b:did_ftplugin	ftpluginruby.vim/ftplugin/ruby.vim	/^let b:did_ftplugin = 1$/;"	v
b:match_ignorecase	ftpluginruby.vim/ftplugin/ruby.vim	/^    let b:match_ignorecase = 0$/;"	v
b:match_words	ftpluginruby.vim/ftplugin/ruby.vim	/^    let b:match_words =$/;"	v
rubypath	ftpluginruby.vim/ftplugin/ruby.vim	/^      let rubypath = system("ruby -e 'print $:.join(%q{,})'" )$/;"	v
rubypath	ftpluginruby.vim/ftplugin/ruby.vim	/^      let rubypath = system('ruby -e "print $:.join(%q{,})"' )$/;"	v
rubypath	ftpluginruby.vim/ftplugin/ruby.vim	/^    let rubypath = ".,,"$/;"	v
rubypath	ftpluginruby.vim/ftplugin/ruby.vim	/^    let rubypath = substitute(rubypath,',.$',',,','')$/;"	v
s:save_cpo	ftpluginruby.vim/ftplugin/ruby.vim	/^let s:save_cpo = &cpo$/;"	v
